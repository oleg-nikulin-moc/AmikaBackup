    height: initial;
    padding: 20px 0px .5rem;
    img {
      max-height: 380px;
      width: 100%;

      &.mobile-only {display: none; @media(max-width: 767px){display: block;}}
      &.desktop-only {display: block; @media(max-width: 767px){display: none;}}
    }
    .table-cell {
      position: relative;
    }
  }

  /* Adjust alignment of PLP filter and content */
  #collection {
    margin: 50px auto 20px;

    .collectionToggle {
      @media(min-width: 901px){margin-top: -30px;}
      #MainContent .product-item {margin-top: 0;}
    }
  }

  /* Side Filter Styling */
  .sid-wrp  {
    @media(max-width: 767px){padding-left: 0;}
    h2.head-coll {margin: initial; padding: initial; margin-bottom: 10px;}
    .sidebar-group.collections ul {margin: initial; padding: initial;}
    .inner-wrapper-sticky {
      border: 1px solid transparent;
      width: 153px !important;
    }
  }


  .sidebar-collection {
    max-width: 153px;
    .sidebar-group, .filter-group {margin-bottom: 25px;}
    .sidebar-group {
      ul li {
        // max-width: 75px;
        position: relative;
      }
    }
    h2, h4 {
      text-transform: lowercase;
      font-size: 16px;
      margin-bottom: 10px;
    }
    ul li {
      margin-bottom: 10px !important;
      line-height: 1;
      &.animation ul.hide {
        margin-top: 15px !important;
        margin-bottom: 15px !important;
        li{max-width: initial; padding-left: 13px;}
      }
      a {
        opacity: 1;
        &.opener {
          position: absolute;
          right: 40%;
          & .arrow {display: none;}
          &::after {
            background:url('https://cdn.shopify.com/s/files/1/2117/1151/files/LA-plus-sign.svg?9863828305200029852');
            background-repeat: no-repeat;
            background-position-y: 55%;
            width: 10px;
            height: 12px;
            content: '';
            display: inline-block;
          }
        }
        &.opener.opened{
          transform: initial;
          &::after {
            background:url('https://cdn.shopify.com/s/files/1/2117/1151/files/LA-minus-sign.svg?9863828305200029852');
            background-repeat: no-repeat;
            background-position: center center;
            width: 10px;
            height: 12px;
            content: '';
            display: inline-block;
          }
        }
      }
    }
  }

  /* Sticky Bar - Stays White on scroll */
  .collection-top {
    &.sticky {
      background: #FEFAF0;

      .breadcrumb {
        .page-title {color: black;}
        .homepage-link, i {color: #aaa;}}
      .sort-select {
        label, select {color: black;}}
    }
  }

  /* Sort By */
  .collection-top  {
    .sort-select {
      // display: none;
      label {
        text-transform: lowercase;
        &:after {
          content: '';
          display: inline-block;
