/*******************/
/* VARIABLES */
/*******************/

// $white : #FEFAF5;
$white : #fff;
$orange : #FF6B26;
$color-text-dark: #000;
$color-text-light: #fff;
$color-off-white: #FEFAF0;
$color-accent: #FF5EDB;
$color-accent-secondary: #FF6B26;
$founders: "Founders Grotesk Mono", helvetica, sans-serif;
$futura: 'FuturaND', helvetica, sans-serif;

$breakpoint-tab: 768px;
$breakpoint-tab-lg: 1024px;
$breakpoint-wide: 1400;

/******************/
/* HELPER CLASSES */
/******************/

.success {
  color: green;
}
.error{
  color: red;
}

.font-primary {
  font-family: $futura;
}
.font-secondary {
  font-family: $founders;
}
.text--dark { color: $color-text-dark !important; }
.text--light { color: $color-text-light !important; }
.fut_200 {font-weight: 200;}
.fut_300 {font-weight: 300;}
.fut_400 {font-weight: 400;}
.fut_500 {font-weight: 500;}
.fut_600 {font-weight: 600;}
.fut_700 {font-weight: 700;}
.fut_800 {font-weight: 800;}

.show-tab,
.show-tab-lg {
  display: none;
}

@media screen and (min-width: $breakpoint-tab) {
  .show-tab {
    display: block;
  }
  .hide-tab {
    display: none;
  }
}

@media screen and (min-width: $breakpoint-tab-lg) {
  .show-tab-lg {
    display: block;
  }
  .hide-tab-lg {
    display: none;
  }
}

