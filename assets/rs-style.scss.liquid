/*************/
/* QUICKVIEW */
/*************/
.tingle-modal.quick-view {
  padding-top:0;
  @media(max-width: 767px){
    padding: 0;
  }

  .yotpoRating {display: none;}
  .badges .title-icon img {width: 50px;}
  #product-wrap {padding: 40px 20px;}
  #product-gallery {position: relative;}
  .tingle-modal-box {
    max-width: initial;
    margin: auto;
    width: 90%;
    @media(min-width: 768px){
      min-width: 302px;
    }
  }
  .tingle-modal__close {
    -webkit-transition: .3s ease;
    -moz-transition: .3s ease;
    transition: .3s ease;
    display: block;
    height: 35px;
    width: 35px;
    border: 1px solid #222;
    border-radius: initial;
    top: 0;
    right: 0;
    color: #444;
    border-right: none;
    border-top: none;
    display: none;

    span.tingle-modal__closeLabel {display: none;}

    &:hover {
      background: #444 !important;
      color: white;
      -webkit-transition: .3s ease;
      -moz-transition: .3s ease;
      transition: .3s ease;
    }
  }
  div#quick-view {
    max-width: 900px;
    margin: auto;
    z-index: 9999;
    position: relative;
    @media(max-width: 767px){width: 90%;}
    @media(min-width:768px){
      -webkit-transform: translateY(2.5%);
      -moz-transform: translateY(2.5%);
      transform: translateY(2.5%);
    }


    button {
      -webkit-transition: .3s ease;
      -moz-transition: .3s ease;
      transition: .3s ease;
      display: block;
      height: 35px;
      width: 35px;
      border: 1px solid #444;
      border-radius: initial;
      top: 0;
      right: 0;
      color: #444;
      border-right: none;
      border-top: none;
      position: absolute;

      &.aclose {
        font-family: "FuturaND";
        font-size: 25px;
      }
    }

    .slick-dots{
      li {margin-right: 10px;}
      button {
        height: 10px;
        width: 10px;
        border-radius: 50%;
        border: 0;
      }
    }
  }
}


