/*******************************************************************************
Banner w/ Overlay
*******************************************************************************/
.banner-overlay__container {
  position: relative;
  padding: 0;
  width: 100%;
  overflow: hidden;
  height: inherit;
  display: flex;
  flex-direction: column;

  .banner-overlay__image--desktop,
  .banner-overlay__video--desktop {
    display: none;
  }

  @media screen and (min-width: 1024px) {
    .banner-overlay__image--desktop,
    .banner-overlay__video--desktop {
      display: block;
    }

    .banner-overlay__image--mobile,
    .banner-overlay__video--mobile {
      display: none;
    }
  }
}

.banner-overlay__text-container {
  padding: 60px 2em;
  text-align: center;

  &.small-text-align--left { text-align: left; }
  &.small-text-align--center { text-align: center; }
  &.small-text-align--right { text-align: right; }

  @media screen and (min-width: 1024px) {
    padding: 3em;

    &.tablet-lg-up-text-align--left { text-align: left; } 
    &.tablet-lg-up-text-align--center { text-align: center; }
    &.tablet-lg-up-text-align--right { text-align: right; }
  }
}

.banner-overlay__media {
  display:flex;
  justify-content: center;
  align-items:flex-start;
  height: 100%;
  width: 100%;

  .banner-overlay__image,
  .banner-overlay__video {
    width: 100%;
    max-width: 100%;
  }
}


.banner-overlay__overlay {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  display: flex;
  justify-content: center;
  align-items: center;

  &.text-h-left--mobile {
    justify-content: flex-start;
  }
  &.text-h-center--mobile {
    justify-content: center;
  }
  &.text-h-right--mobile {
    justify-content: flex-end;
  }
  &.text-v-top--mobile {
    align-items: flex-start;
  }
  &.text-v-center--mobile {
    align-items: center;
  }
  &.text-v-bottom--mobile {
    align-items: flex-end;
  }

  &.v-top--mobile.h-left--mobile {
    top: 0;
    left: 0;
    transform: translate(0, 0);
  }
  &.v-top--mobile.h-center--mobile {
    top: 0;
    left: 50%;
    transform: translate(-50%, 0);
  }
  &.v-top--mobile.h-right--mobile {
    top: 0;
    left: 100%;
    transform: translate(-100%, 0);
  }
  &.v-center--mobile.h-left--mobile {
    top: 50%;
    left: 0%;
    transform: translate(0, -50%);
  }
  &.v-center--mobile.h-center--mobile {
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);

  }
  &.v-center--mobile.h-right--mobile {
    top: 50%;
    left: 100%;
    transform: translate(-100%, -50%);
  }
  &.v-bottom--mobile.h-left--mobile {
    top: 100%;
    left: 0%;
    transform: translate(0, -100%);
  }
  &.v-bottom--mobile.h-center--mobile {
    top: 100%;
    left: 50%;
    transform: translate(-50%, -100%);
  }
  &.v-bottom--mobile.h-right--mobile {
    top: 100%;
    left: 100%;
    transform: translate(-100%, -100%);
  }

  @media screen and (min-width: 1024px) {
    &.text-h-left {
      justify-content: flex-start;
    }
    &.text-h-center {
      justify-content: center;
    }
    &.text-h-right {
      justify-content: flex-end;
    }
    &.text-v-top {
      align-items: flex-start;
    }
    &.text-v-center {
      align-items: center;
    }
    &.text-v-bottom {
      align-items: flex-end;
    }
    &.v-top.h-left {
      top: 0;
      left: 0;
      transform: translate(0, 0);
    }
    &.v-top.h-center {
      top: 0;
      left: 50%;
      transform: translate(-50%, 0);
    }
    &.v-top.h-right {
      top: 0;
      left: 100%;
      transform: translate(-100%, 0);
    }
    &.v-center.h-left {
      top: 50%;
      left: 0%;
      transform: translate(0, -50%);
    }
    &.v-center.h-center {
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
    }
    &.v-center.h-right {
      top: 50%;
      left: 100%;
      transform: translate(-100%, -50%);
    }
    &.v-bottom.h-left {
      top: 100%;
      left: 0%;
      transform: translate(0, -100%);
    }
    &.v-bottom.h-center {
      top: 100%;
      left: 50%;
      transform: translate(-50%, -100%);
    }
    &.v-bottom.h-right {
      top: 100%;
      left: 100%;
      transform: translate(-100%, -100%);
    }
  }
}

@mixin resetPosition {
  top: unset;
  left: unset;
  transform: none;
}

@media screen and (max-width: 1023px) {
  .stacked--mobile {
    .banner-overlay__overlay {
      @include resetPosition;
      width: 100%;
      height: auto;
      position: relative;
  
      &.v-top--mobile.h-left--mobile {
        @include resetPosition;
      }
      &.v-top--mobile.h-center--mobile {
        @include resetPosition;
  
      }
      &.v-top--mobile.h-right--mobile {
        @include resetPosition;
  
      }
      &.v-center--mobile.h-left--mobile {
        @include resetPosition;
  
      }
      &.v-center--mobile.h-center--mobile {
        @include resetPosition;
  
      }
      &.v-center--mobile.h-right--mobile {
        @include resetPosition;
  
      }
      &.v-bottom--mobile.h-left--mobile {
        @include resetPosition;
  
      }
      &.v-bottom--mobile.h-center--mobile {
        @include resetPosition;
  
      }
      &.v-bottom--mobile.h-right--mobile {
        @include resetPosition;
      }
    }
  }
}

@media screen and (min-width: 1024px) {
  .stacked {
    .banner-overlay__overlay {
      width: 100%;
      height: auto;
      position: relative;
      position: relative;
      top: unset;
      left: unset;
      transform: none;
    }
  }
}

