div#dd {
  position: relative;
  display: inline-block;
  float: right;
  z-index: 9;
  text-align: right;
  padding-bottom: 0;
  padding-top: 0;
  margin-bottom: 1rem;
}
.wrapper-dropdown {
  span.sort_label {
    text-transform: lowercase;
    font-size: 12px;
    font-weight: normal;
  }
  &.active .dropdown {
    opacity: 1;
    pointer-events: auto;
    list-style: none;

    &.active:after {
      border-color: #9bc7de transparent;
      border-width: 6px 6px 0 6px ;
      margin-top: -3px;
    }
  }
  /* Size and position */
  position: relative; /* Enable absolute positioning for children and pseudo elements */
  width: 200px;
  padding: 10px;
  margin: 0 auto;

  /* Styles */
  color: black;
  outline: none;
  cursor: pointer;

  /* Font settings */
  font-weight: bold;

  &:after {
    content: '';
    display: inline-block;
    width: 10px;
    height: 10px;
    background: url('https://cdn.shopify.com/s/files/1/2117/1151/files/LA-arrow-down.svg?12998433554777743291');
    background-repeat: no-repeat;
    background-size: contain;
    background-position-y: 50%;
    margin-left: 10px;
  }

  .dropdown {
    /* Size & position */
    position: absolute;
    top: 100%;
    left: 0; /* Size */
    right: 0; /* Size */
    /* Styles */
    background: #fefaf0;
    text-align: right;
    font-weight: normal;
    /* Hiding */
    opacity: 0;
    pointer-events: none;

    li {padding-right: 30px; font-size: 12px;}
    li a {
      display: block;
      text-decoration: none;
      color: black;
      padding: 0 30px;
    }
    li:last-of-type, option:last-of-type {padding-bottom: 15px;}
    li:hover a, option:hover a {text-decoration: underline; opacity: 1;}
    option {font-size: 12px;}
  }
}

