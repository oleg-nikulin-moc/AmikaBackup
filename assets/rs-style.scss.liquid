/*****************/
/* GATEWAY POPUP */
/*****************/
#shopify-section-bf-popup {
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.75);
  position: fixed;
  z-index: 9999;
  opacity: 0;

  &.trans {
    opacity: 1;
    -webkit-transition: 1s;
    -moz-transition: 1s;
    transition: 1s;
  }

  .bf_container {
    max-width: 550px;
    padding: .5rem;
    text-align: center;
    background: #a57eb9;
    position: relative;
    color: white;
    @media(min-width:768px){
      left: 50%;
      margin-left: -275px;
      top: 50%;
      transform: translateY(-50%);
      display: table;

      .bf_content_container {
        display: table-cell;
        vertical-align: middle;
      }
    }

    .color {color:#e86050;}
    p {color: white; font-size: 1.2rem;}
    h2 {
      font-family: "abril-text",sans-serif;
      font-weight: 600;
      max-width: 420px;
      margin: auto;
      font-size: 1.9rem;
    }
  }
  .bf__content {
    max-width: 470px;
    margin: auto;

    p:nth-of-type(3) {
      max-width: 285px;
      margin: auto;
    }
  }

  /*  Mailing List */
  .mailing-list {
    text-align: center;
    padding: 2rem 2rem 1rem;
    border: 1px solid #ccc;

    .title {
      margin-bottom: 16px;
      color: white;
      text-transform: lowercase;
      font-size: 21px;
      font-family: 'FuturaND';
      font-weight: 500;
    }
    form {
      display: inline-block;
      width: 100%;
      max-width: 450px;
      padding: 10px 10px 10px 5px;
      border-bottom: 2px solid #EAEAEA;
      margin-top:10px;
      box-sizing:border-box;
      /*       background:#f0f0f0; */
      background:transparent;
    }
    form input[type="email"] {
      width: 74%;
      color: black;
      border: none;
      padding: 0;
      font-size: 18px;
      letter-spacing: 0.05em;
    }
    form input[type="submit"] {
      width: 24%;
      color: black;
      text-align: right;
      padding: 0;
      font-weight: bold;
      font-size: 18px;
      font-family: 'FuturaND';
    }
    .success {
      color: green;
      padding: 5px 0;
    }
    .login {
      margin: 1rem 0 0;
      font-size: 14px !important;

      a {
        text-decoration: underline;
        display: block;
      }
    }

    @media (max-width: 900px){
      padding: 20px;
      .title {font-size:18px;}
      form {
        font-size: 0;
        letter-spacing: 0;

        input[type="email"]{font-size: 16px;width:70%;}
        input[type="submit"]{font-size: 16px;font-weight: 500;width: 30%;}
      }
    }


    /* Create account */
    #customer-register {
      float: none;
      width: 100%;
      color: #000000;

      form {
        border-bottom: 0;

        input[type="submit"] {
          width: 50%;
          color: white;
          text-align: center;
          padding: 0;
          font-weight: bold;
          font-size: 18px;
          font-family: 'FuturaND';
          border: 1px solid white;
          padding: .5rem 1rem;
        }
      }
    }
  }
}


