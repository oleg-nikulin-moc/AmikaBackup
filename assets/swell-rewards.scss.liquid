          border-bottom: 1px solid #ececec;
        }
        .accordian{
          overflow: hidden;
          position: relative;
          li{
            list-style: none;
            border-bottom: none;
            &.question{
              line-height: 1.5;
              padding: 10px 0;
              cursor: pointer;
              display: flex;
              justify-content: space-between;
              align-items: flex-start;
              text-align: left;  
              .para{
                font-size: 18px;
              }
              .cross{
                position: absolute;
                right: 5px;
                bottom: 5px;
                color: #cccccc;
                -webkit-transition: all 300ms ease-in-out;
                -moz-transition: all 300ms ease-in-out;
                transition: all 300ms ease-in-out; 
                -webkit-transform: rotate(90deg);
                -moz-transform: rotate(90deg);
                -ms-transform: rotate(90deg);
                transform: rotate(90deg);
                i{
                  font-size: 12px;
                  display: block;
                }
              }
              &.open{
                .cross{
                  right: 3px;
                  display: block;
                  -webkit-transform: rotate(-90deg);
                  -moz-transform: rotate(-90deg);
                  -ms-transform: rotate(-90deg);
                  transform: rotate(-90deg);
                  margin-right: 1px;
                  i{
                    color: #ff486b;
                  }
                }
              }
            }
            &.answer{
              font-size: 13px;
              line-height: 1.4;
              opacity: 0;
              height: 0;
              margin: 0;
              -webkit-transition: all 0.3s ease;
              -moz-transition: all 0.3s ease;
              transition: all 0.3s ease;
              text-align: left;
              &.open{
                opacity: 1;
                height: auto;
                margin: 0 0 15px 0;
              }
            }
          }
        }
      }
    }
  }
