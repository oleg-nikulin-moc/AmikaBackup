$min: min-width;
$max: max-width;
$small: 550px;
$s-medium: 767px;
$medium: 996px;
$large: 1025px;
$x-large: 1200px;

/*Color Variables Starts*/


/*Text colors*/

$colorTextWhite: #fff;
$colorTextBlack: #000;
$colorTextPink: #ff7376;

/*Text colors End*/

/*font-family Variables*/

$futura: 'Futura Md BT';

@font-face {
  font-family: 'swell-rewards-icons';
  src:  url('swell-rewards-icons.eot?qs7jsv');
  src:  url('swell-rewards-icons.eot?qs7jsv#iefix') format('embedded-opentype'),
    url('swell-rewards-icons.ttf?qs7jsv') format('truetype'),
    url('swell-rewards-icons.woff?qs7jsv') format('woff'),
    url('swell-rewards-icons.svg?qs7jsv#swell-rewards-icons') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: block;
}

[class^="swell-icon-"], [class*=" swell-icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'swell-rewards-icons' !important;
  speak: never;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.swell-icon-Purchases:before {
  content: "\e900";
}
.swell-icon-CelebrateBirthday:before {
  content: "\e901";
}
.swell-icon-PlaceAnOrder:before {
  content: "\e902";
}
.swell-icon-ReferFriendIcon:before {
  content: "\e903";
}
.swell-icon-Spend200:before {
  content: "\e904";
}
.swell-icon-WriteAReview:before {
  content: "\e905";
}


@font-face {
  font-family: $futura;
  src: url('FuturaBT-Medium.woff2') format('woff2'), url('FuturaBT-Medium.woff') format('woff');
  font-weight: 500;
  font-style: normal;
}


.swell-loader {
  width: 60px;
  background: transparent !important;
  animation: none !important;
}

.select2-container {
  z-index: 999999;
  width: 100% !important;
}

.swell-cart-content {
  display: flex;
  flex-direction: column;
  .swell-points {
    text-align: center;
    .swell-error {
      color: red;
    }
  }
  .dropdown-holder {
    width: 100%;
    .select2-container {
      .select2-selection {
        display: flex !important;
        align-items: center;
        justify-content: center;
        height: 40px;
        .swell-custom-list-option {
          display: flex;
          align-items: center;
          justify-content: center;
          span {
            display: flex !important;
            align-items: center;
            justify-content: center;
          }
        }
      }
      img {
        max-width: 28px;
      }
      .option-text {
        font-size: 12px;
        text-transform: capitalize;
      }
    }
  }
}

.swell-orange {
  color: #ff6b26 !important;
}

.jconfirm-type-green {
  .jconfirm-buttons {
    .btn {
      &:first-child {
        display: none;
      }
    }
  }
}

.apply-button {
  cursor: pointer;
  background-color: #000;
  color: #fff !important;
  max-width: 200px;
  width: 100%;
  display: inline-block;
  border-radius: 5px;
  padding: 10px 0;
  font-size: 14px;
  &.apply-button-cart {
    border-radius: 35px;
    max-width: 600px;
    margin-top: 15px;
    height: 53px;
    font-size: 16px;
    font-weight: 600;
    display: flex;
    align-items: center;
    justify-content: center;
  }
}

.select2-container--default {
  .select2-selection--single {
    .select2-selection__arrow {
      top: 8px;
    }
  }
}

.select2-dropdown {
  .swell-custom-list-option {
    .option-text {
      font-size: 12px !important;
    }
  }
}

.swell-custom-list-option {
  img {
    max-width: 20px;
  }
}

.swell-products {
  position: relative;
}
.ways-to-earn-tier {
  position: absolute;
  z-index: 999;
  left: 0;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
  max-width: 440px;
  @media(max-width: 766px) {
    max-width: 395px;
  }
  @media(max-width: 549px) {
    max-width: 315px;
  }
  @media(max-width: 480px) {
    max-width: 250px;
  }
}
.amika-rewards {
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background: #fffffffa;
  z-index: 9998;
  display: none;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  padding: 0 15px;
  @media($min: $small){
    padding: 0 30px;
  }
  @media($min: $s-medium){
    padding: 0;
  }
  .checkout-heading{
    h2{
      font-family: $futura;
      font-size: 20px;
      color: $colorTextBlack;
      text-transform: lowercase;
      margin: 0 0 10px;
      text-align: center;
      max-width: 140px;
      @media($min: $small){
        max-width: 150px;
      }
      @media($min: $s-medium){
        margin: 0 0 25px;
      }
    }
    .swell-referral-back-link.checkout{
      top: -40px;
      width: 20px;
      @media($min: $large){
        top: -50px;
        width: 2em;
      }
      &::before, &::after{
        background-color: $colorTextBlack;
        max-width: calc(20px * 1.414);
        @media($min: $large){
          max-width: calc(24px * 1.414);
        }
      }
    }
  }
  .amika-post-checkout{
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    position: relative;
    .referral-holder{
      .swell-referral{
        padding: 0;
        margin-bottom: 0;
        .swell-referral-thanks{
          .swell-referral-back-link{
            &:before, &:after{
              background-color: $colorTextBlack;
            }
          }
          .swell-referral-heading{
            color: $colorTextBlack;
          }
          .swell-referral-details{
            color: $colorTextBlack;
          }
        }
        .swell-referral-content-container{
          .swell-referral-content-sidebar{
            .swell-referral-sidebar-details{
              margin-bottom: 1vw;
              .swell-referral-sidebar-details-action{
                color: $colorTextBlack;
              }
            }
          }
          .swell-referral-content-main{
            .swell-referral-refer{
              .swell-referral-heading{
                color: $colorTextBlack;
                margin: 2.5vw 0;
                @media($min: $s-medium){
                  margin: 1vw 0 1.4vw;
                }
              }
              .swell-referral-details-container{
                border-bottom: 1px solid $colorTextBlack;
                margin: 0 auto 1.6vw;
                padding: 0 0 1.6vw;
                @media($min: $s-medium){
                  max-width: 27vw;
                  margin: 0 auto 1.8vw;
                  padding: 0 0 1.8vw;
                }
                .swell-referral-details{
                  color: $colorTextBlack;
                  margin: 0 auto 0;
                }
              }
              .swell-referral-form-wrapper{
                .swell-referral-form{
                  .swell-referral-form-header{
                    .swell-referral-form-header-details{
                      color: $colorTextBlack;
                    }
                  }
                  .swell-referral-form-body{
                    .refer-customer-error{
                      color: red;
                    }
                    .swell-referral-form-list{
                      background-color: $colorTextWhite;
                      border-radius: 30px;
                      margin: 2.6vw auto;
                      @media($min: $s-medium){
                        margin: 2vw auto 1vw;
                        flex-direction: row;
                      }
                      .swell-referral-form-list-field {
                        .swell-referral-form-list-field-input {
                          background-color: #f1f1f1;
                          &.error-border{
                            border: 1px solid red;
                          }
                        }
                        .swell-referral-form-list-submit {
                          border: 2px solid $colorTextBlack;
                          box-sizing: border-box;
                        }
                      }
                      .swell-referral-form-list-button{
                        .swell-referral-form-list-submit{
                          border: 2px solid $colorTextBlack;
                          box-sizing: border-box;
                        }
                      }
                    }
                  }
                }
              }
              .swell-referral-media-wrapper{
                .swell-referral-media-details{
                  color: $colorTextBlack;
                }
                .swell-referral-media-list{
                  margin: 3vw auto 0;
                  @media($min: $s-medium){
                    margin: 0.8vw auto 0;
                  }
                  .swell-referral-medium{
                    .swell-referral-medium-content{
                      color: $colorTextBlack;
                      i{
                        color: $colorTextBlack;
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
