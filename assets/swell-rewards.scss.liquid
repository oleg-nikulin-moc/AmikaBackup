.rewards-page {
  .swell-products {
    text-align: center;
    .slick-list.draggable {
      overflow-y: hidden;
    }
    .tier1-section, .tier2-section, .tier3-section{
      margin: 30px 0;
      position: relative;
      @media($min: $small){
        margin: 40px 0;
      }
      @media($min: $s-medium){
        margin: 50px ;
      }
      @media($min: $large){
        margin: 60px 0;
      }
      @media($min: $x-large){
        margin: 0 70px 70px 70px;
      }
      .disable-carousel{
        width: 100%;
        height: 100%;
        position: absolute;
        top: 0;
        background-color: #ffffffa3;
      }
      &.swell-disable {
        display: none;
      }
      h2{
        color: #000;
        font-family: $futura;
        margin: 0;
        font-size: 23px;
        @media($min: $small){
          font-size: 36px;
        }
        @media($min: $s-medium){
          font-size: 32px;
        }
        @media($min: $large){
          font-size: 44px;
        }
        @media($min: $x-large){
          font-size: 52px;
        }
      }
      p{
        color: $colorTextBlack;
        font-family: $futura;
        line-height: 1.3;
        margin: 10px 0 0;
        font-size: 11px;
        @media($min: $small){
          margin: 30px 0 0;
          font-size: 19px;
        }
        @media($min: $s-medium){
          margin: 15px 0 0;
          font-size: 13px;
        }
        @media($min: $large){
          font-size: 18px;
        }
        @media($min: $x-large){
          margin: 20px 0 0;
          font-size: 21px;
        }
      }
      .ways-to-earn-tier {
        margin: 0 auto;
        opacity: 1;
        @media(max-width: 549px) {
          font-size: 15px;
        }
        @media(max-width: 480px) {
          font-size: 12px;
        }
        /*@media(max-width: 360px) {
          font-size: 14px;
        }*/
        a {
          text-decoration: underline;
        }
      }
      .tier1-redeemable, .tier2-redeemable, .tier3-redeemable{
        max-width: 1200px;
        padding: 30px 20px 10px;
        list-style: none;
        margin:  0 auto;
        position: relative;
        @media($min: $small){
          padding: 40px 20px 20px;
        }
        @media($min: $s-medium){
          padding: 30px 30px 10px;
        }
        @media($min: $large){
          padding: 40px 30px 20px;
        }
        @media($min: $x-large){
          padding: 50px 30px 30px;
        }
        .slick-arrow-left{
          left: 5px;
          color: #000;
        }
        .slick-arrow-right{
          right: 5px;
          color: #000;
        }
        h4, h5, h6{
          color: $colorTextBlack;
          text-align: center;
          font-family: $futura;
        }
        .slick-list{
          .slick-track{
            padding-top:7vw;
            @media($min: $s-medium){
              padding-top: 0;
            }
          }
        }
        .swell-redemption-option{
          margin: 0;
          @media($min: $s-medium){
            margin: 0 10px;
          }
          &.slick-center{
            position: relative;
            @media($max: 766px){
              -moz-transform: scale(1);
              -ms-transform: scale(1);
              -o-transform: scale(1);
              -webkit-transform: scale(1);
              opacity: 1;
              z-index: 9900;
              transform: scale(1);
              transition: all 300ms ease;
            }
            .swell-buy-product-btn-container{
              img{
                @media($max: 766px){
                  width: 100%;
                  transform: scale(1.4);
                  margin-bottom: 40px;
                  @media($min: $small){
                    margin-bottom: 70px;
                  }
                }
              }
            }
          }
          &:not(.slick-center){
            @media($max: 766px){
              opacity: 0.4;
              -moz-transform: scale(0.75);
              -ms-transform: scale(0.75);
              -o-transform: scale(0.75);
              -webkit-transform: scale(0.75);
              transform: scale(0.75);
              transition: all 300ms ease;
            }
          }
          .swell-buy-product-btn-container{
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
            &:hover{
              .swell-redemption-option-content{
                .redeem-overlay{
                  opacity: 1;
                }
              }
            }
            .swell-redemption-option-content{
              position: relative;
              margin-bottom: 30px;
              @media($min: $small){
                margin-bottom: 20px;
              }
              img{
                width: 100%;
                display: block;
                max-width: 290px;   
              }
              .redeem-overlay{
                display: none;
                justify-content: center;
                flex-direction: column;
                position: absolute;
                width: 100%;
                bottom: 0;
                -webkit-transition: all 300ms ease-in-out;
                -moz-transition: all 300ms ease-in-out;
                transition: all 300ms ease-in-out;
                opacity: 0;
                .hide{
                  display: none !important;
                }
                a{
                  font-family: $futura;
                  font-weight: 500;
                  color: $colorTextBlack;
                  text-transform: lowercase;
                  text-align: center;
                  text-decoration: underline;
                  background-color: rgba(255, 255, 255, 0.7490196078431373);
                  font-size: 10px;
                  padding: 0;
                  opacity: 1;
                  @media($min: $small){
                    font-size: 13px;
                    padding: 3px 0;
                  }
                }
              }
            }
            .title{
              margin-bottom: 5px;
              @media($min: $small){
                margin-bottom: 10px;
              }
              @media($min: $s-medium){
                margin-bottom: 7px;
              }
              @media($min: $large){
                margin-bottom: 10px;
              }
              h4{
                font-weight: normal;
                margin: 0;
                font-size: 14px;
                @media($min: $small){
                  font-size: 21px;
                }
                @media($min: $s-medium){
                  font-size: 18px;
                }
                @media($min: $large){
                  font-size: 21px;
                }
              }
            }
            .description{
              h6{
                font-weight: lighter;
                margin: 0;
                font-size: 9px;
                @media($min: $small){
                  font-size: 12px;
                }
                @media($min: $s-medium){
                  font-size: 11px;
                }
                @media($min: $large){
                  font-size: 12px;
                }
              }
            }
            .points{
              margin: 12px 0 0;
              @media($min: $small){
                margin: 20px 0 0;
              }
              @media($min: $s-medium){
                margin: 15px 0 0;
              }
              @media($min: $large){
                margin: 20px 0 0;
              }
              h5{
                font-weight: lighter;
                color: #ff6b26;
                font-size: 10px;
                @media($min: $small){
                  font-size: 18px;
                }
                @media($min: $s-medium){
                  font-size: 14px;
                }
                @media($min: $large){
                  font-size: 18px;
                }
              }
            }
          }
        }
      }
    }
    .swell-disable {
      display: none;
    }

    .earning-container {
      .swell-show-few-product-redemptions {
        cursor: pointer;
        width: 100%;
        background-color: transparent;
        font-family: "Futura Md BT";
        color: #000;
        text-decoration: none;
        max-width: 100%;
        border: 2px solid;
        margin: 0 auto 15px;
        display: block;
        max-width: 620px;
