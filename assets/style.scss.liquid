// Button Styles

.button{
  text-transform: uppercase;
  text-align: center;
  border:1px solid;
  display:inline-block;
  appearance: none;
  -webkit-appearance: none;
}

.button--rounded{
  border-radius: 100px;
}

.button--large{
  @extend button;
  font-weight: 500;
  font-size: 0.875rem;
  line-height: 18px;
  padding:13px 20px;

  @media(max-width:1024px){
    font-size:0.6875rem;
  }
}

.button--small{
  @extend button; 
  font-size:14px;
  line-height: 14px;
  letter-spacing: 1px;
  padding: 10px 35px 8px;
  font-weight: 500;

  @media(max-width:768px){
    font-size:11px;
    line-height:11px;
  }
}

.button--fullWidth{
  width: 100%;
  display: block;
  box-sizing: border-box;
}


.button--primary{
  @extend .button;
  @extend .button--rounded; 
  @extend .button--large; 
  background-color:#000;
  border-color:#000;
  color: #FEFAF0;

  &:hover, 
  &:focus{
    background: #FEFAF0;
    color:#000;
    opacity: 1;
  } 
}

.button--secondary{
  @extend .button;
  @extend .button--rounded; 
  @extend .button--small; 
  background-color:#FEFAF0;
  border-color:#000;
  color: #000;

  &:hover, 
  &:focus{
    background: #000;
    color:#FEFAF0;
    opacity: 1 !important;
  }
}

// Overrides
#customer-login form input[type="submit"], 
#customer-register form input[type="submit"]{
 @extend .button--secondary;
}

