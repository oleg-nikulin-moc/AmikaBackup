#searchresults p.note {margin: 100px 0 40px 0; text-align: center; }
#searchresults .search-form { margin: 0 auto 100px auto;}
#searchresults .search-form form { box-sizing: border-box; width: 100%; max-width: 500px; padding: 3px 10px 5px;border: 1px solid #000;background: #fdfaf0;}
#searchresults .search-form form input[type="text"] { width: 80%; color: #000; border: none; padding: 0; font-size: 15px; letter-spacing: 0.05em;vertical-align: -webkit-baseline-middle;}
#searchresults .search-form form input[type="submit"] { width: 18%; color: #fff; margin-top: 4px;text-align: right; padding: 0;}
#searchresults .search-form form button {
  background: transparent;
  vertical-align: -4px;
  margin: 0 auto;
  margin-top: 5px;
  display: inline;
  float: right;
  line-height: 1;
}

#searchresults .search-form form {
  .onhover__search {
    display: none;
  }

  &:hover {
    .onhover__search {
      display: block;
    }

    .not_onhover__search {
      display: none;
    }
  }
}

