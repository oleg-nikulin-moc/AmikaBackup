<section data-section-id="{{ section.id }}" data-section-type="terra-cycle">
  <div class="terra__cycle">
    
    <!-- FIRST SECTION -->
    <div class="container-fluid max-width-container">      <div class="banner__section">
        <div class="banner__content">
          {% if section.settings.first_section_heading != blank %}
            <h3 class="text--heading">
              {{ section.settings.first_section_heading }}
            </h3>
          {% endif %}
          {% if section.settings.first_section_paragraph != blank %}
            <div class="text--paragraph">
              {{ section.settings.first_section_paragraph }}
            </div>
          {% endif %}

          {% if section.settings.first_section_btn1 != blank %}
            <div class="text--buttons">
              <a class=" button--primary"
			  	       target="_blank"
                 href="{{ section.settings.first_section_btn1_url }}">
                {{ section.settings.first_section_btn1 }}
              </a>
            </div>
          {% endif %}
        </div>
        <div class="banner__media">
          {% if section.settings.enable_first_section_video %}
            {% if section.settings.first_section_video != blank %}
              <video width="100%" height="100%">
                <source src="{{ section.settings.first_section_video }}" type="video/mp4">
              </video>
            {% endif %}
          {% else %}
            {% if section.settings.first_section_image_d != nil %}
              {% assign image_d = section.settings.first_section_image_d %}
              {% assign image_m = section.settings.first_section_image_m | default: section.settings.first_section_image_d %}

              <img src="{{ image_d | img_url: 'master' }}" class="image--desktop" alt="Image" />
              <img src="{{ image_m | img_url: 'master' }}" class="image--mobile" alt="Image" />
            {% endif %}
          {% endif %}
        </div>
      </div>
    </div>
    
    <!-- QUIZ BUTTON -->
    <div class="container-fluid max-width-container">
      <div class="quiz__button">
        <a href="/pages/ace" target="_blank">
          hair IQ
        </a>
      </div>
    </div>
    
	<!-- SECOND SECTION -->
    <div class="image__overlay">
      <div class="overlay__media">
        {% if section.settings.enable_second_section_video %}
          {% if section.settings.second_section_video != blank %}
        	<video width="100%" height="100%">
        	  <source src="{{ section.settings.second_section_video }}" type="video/mp4">
        	</video>
          {% endif %}
        {% else %}
          {% if section.settings.second_section_image_d != nil %}
        	{% assign image_d = section.settings.second_section_image_d %}
        	{% assign image_m = section.settings.second_section_image_m | default: section.settings.second_section_image_d %}
        
        	<img src="{{ image_d | img_url: 'master' }}" class="image--desktop" alt="Image" />
        	<img src="{{ image_m | img_url: 'master' }}" class="image--mobile" alt="Image" />
          {% endif %}
        {% endif %}
      </div>
      <div class="overlay__content">
        <div class="text__wrapper">
          {% if section.settings.second_section_heading != blank %}
            <h3 class="text--heading">
              {{ section.settings.second_section_heading }}
            </h3>
          {% endif %}
          {% if section.settings.second_section_paragraph != blank %}
            <div class="text--paragraph">
              {{ section.settings.second_section_paragraph }}
            </div>
          {% endif %}
          
          {% if section.settings.second_section_btn1 != blank or section.settings.second_section_btn2 != blank %}
            <div class="text--buttons">
              {% if section.settings.second_section_btn1 != blank %}
                <a class="text--button button--pink"
				           target="_blank"
                   href="{{ section.settings.second_section_btn1_url }}">
              	  {{ section.settings.second_section_btn1 }}
              	</a>
              {% endif %}
              {% if section.settings.second_section_btn2 != blank %}
                <a class="text--button button--orange"
        				   target="_blank"
                   href="{{ section.settings.second_section_btn2_url }}">
              	  {{ section.settings.second_section_btn2 }}
              	</a>
              {% endif %}
            </div>
          {% endif %}
        </div>
      </div>
    </div>
    
    <!-- THIRD SECTION -->
    <div class="container-fluid max-width-container">
      <div class="image__flex">
        <div class="flex__item">
          {% if section.settings.enable_third_section_video %}
            {% if section.settings.third_section_video != blank %}
              <video width="100%" height="100%">
                <source src="{{ section.settings.third_section_video }}" type="video/mp4">
              </video>
            {% endif %}
          {% else %}
            {% if section.settings.third_section_image_d != nil %}
              {% assign image_d = section.settings.third_section_image_d %}
              {% assign image_m = section.settings.third_section_image_m | default: section.settings.third_section_image_d %}

              <img src="{{ image_d | img_url: 'master' }}" class="image--desktop" alt="Image" />
              <img src="{{ image_m | img_url: 'master' }}" class="image--mobile" alt="Image" />
            {% endif %}
          {% endif %}
        </div>
        <div class="flex__item">
          {% if section.settings.third_section_heading != blank %}
            <h3 class="text--heading">
              {{ section.settings.third_section_heading }}
            </h3>
          {% endif %}
          {% if section.settings.third_section_paragraph != blank %}
            <div class="text--paragraph">
              {{ section.settings.third_section_paragraph }}
            </div>
          {% endif %}
        </div>      
      </div>
    </div>
    
    <!-- MARQUEE -->
    <div class="marquee__wrapper">
      <div class="marquee__flex">        
        {% for i in (1..10) %}
          <div class="marquee__element">
            <span>a friend to the planet</span>
		  </div>
		  <div class="marquee__element element__icon">
			<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="82" height="64" viewBox="0 0 82 64">
			  <image id="Vector_Smart_Object" data-name="Vector Smart Object" width="81" height="64" xlink:href="data:img/png;base64,"/>
			</svg>
		  </div>
          <div class="marquee__element">
            <span>going cleaner and greener</span>
          </div>
		  <div class="marquee__element element__icon">
			<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="82" height="64" viewBox="0 0 82 64">
			  <image id="Vector_Smart_Object" data-name="Vector Smart Object" width="81" height="64" xlink:href="data:img/png;base64,"/>
			</svg>
		  </div>
        {% endfor %}
      </div>
    </div>
    
    <!-- FOURTH SECTION -->
    <div class="container-fluid mobile-padding max-width-container">
      <div class="howto__container">
        {% if section.settings.fourth_section_heading != blank %}
          <h3 class="text--heading">
            {{ section.settings.fourth_section_heading }}
          </h3>
        {% endif %}

        {% if section.blocks.size > 0 %}
          <div class="step__flex">
            {% for block in section.blocks %}
              <div class="flex__item">
				{% if block.settings.show_video and block.settings.video_url != blank %}
				  <div class="video__wrapper">
					<video autoplay loop muted playsinline>
					  <source src="{{ block.settings.video_url }}" type="video/mp4">
					</video>
				  </div>
				{% else %}
				  {% if block.settings.image != nil %}
					<img src="{{ block.settings.image | img_url: 'master' }}" alt="Image" />
				  {% endif %}
				{% endif %}
                
                <div class="item--content">
                  {% if block.settings.heading != blank %}
                    <h4 class="block--heading">
                      {{ block.settings.heading }}
                    </h4>
                  {% endif %}
                  {% if block.settings.paragraph != blank %}
                    <div class="text--paragraph">
                      {{ block.settings.paragraph | replace: '<a', '<a target="_blank"' }}
                    </div>
                  {% endif %}
                </div>
              </div>
            {% endfor %}
          </div>
        {% endif %}
      </div>
    </div>
    
    <!-- MARQUEE -->
    <div class="marquee__wrapper second">
      <div class="marquee__flex">        
        {% for i in (1..10) %}
          <div class="marquee__element">
            <span>a friend to the planet</span>
		  </div>
		  <div class="marquee__element element__icon">
			<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="82" height="64" viewBox="0 0 82 64">
			  <image id="Vector_Smart_Object" data-name="Vector Smart Object" width="81" height="64" xlink:href="data:img/png;base64,"/>
			</svg>
		  </div>
          <div class="marquee__element">
            <span>going cleaner and greener</span>
		  </div>
		  <div class="marquee__element element__icon">
			<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="82" height="64" viewBox="0 0 82 64">
			  <image id="Vector_Smart_Object" data-name="Vector Smart Object" width="81" height="64" xlink:href="data:img/png;base64,"/>
			</svg>
		  </div>
        {% endfor %}
      </div>
    </div>
    
    <!-- FIFTH SECTION -->
    <div class="imagetext__flex">
      <div class="flex__item">
        {% if section.settings.fifth_third_section_video %}
          {% if section.settings.fifth_section_video != blank %}
        	<video width="100%" height="100%">
        	  <source src="{{ section.settings.fifth_section_video }}" type="video/mp4">
        	</video>
          {% endif %}
        {% else %}
          {% if section.settings.fifth_section_image_d != nil %}
        	{% assign image_d = section.settings.fifth_section_image_d %}
        	{% assign image_m = section.settings.fifth_section_image_m | default: section.settings.fifth_section_image_d %}
        
        	<img src="{{ image_d | img_url: 'master' }}" class="image--desktop" alt="Image" />
        	<img src="{{ image_m | img_url: 'master' }}" class="image--mobile" alt="Image" />
          {% endif %}
        {% endif %}
      </div>
      <div class="flex__item">
        {% if section.settings.fifth_section_text != blank %}
          <div class="text--paragraph">
            {{ section.settings.fifth_section_text }}
          </div>
        {% endif %}
      </div>  
    </div>
    
  </div>
</section>

<style>
  body #header,
  body .content-wrapper {
	  background: #FEFAF0;
  }
  body #footer {
	margin-top: 50px;
  }
</style>


{% schema %}
  {
    "name": "Terra Cycle",
    "settings": [
	  {
		"type": "header",
		"content": "Section: 1"
	  },
	  {
		"type": "checkbox",
		"id": "enable_first_section_video",
		"label": "Show video",
		"default": false
	  },
	  {
		"type": "url",
		"id": "first_section_video",
		"label": "Video URL"
	  },
	  {
		"type": "image_picker",
		"id": "first_section_image_d",
		"label": "Image (Desktop)"
	  },
	  {
		"type": "image_picker",
		"id": "first_section_image_m",
		"label": "Image (Mobile)"
	  },
	  {
		"type": "text",
		"id": "first_section_heading",
		"label": "Heading",
		"default": "TerraCycle x amika"
	  },
	  {
		"type": "richtext",
		"id": "first_section_paragraph",
		"label": "Paragraph",
		"default": "<p>80% of the 35 million tons of plastic the US produces each year ends up in landfills or oceans. to reduce amika's virgin plastic production, we've partnered with TerraCycle to create a free recycling program for all our packaging</p>"
	  },
	  {
		"type": "text",
		"id": "first_section_btn1",
		"label": "Button text",
		"default": "JOIN NOW"
	  },
	  {
		"type": "url",
		"id": "first_section_btn1_url",
		"label": "Button link"
	  },
	  {
		"type": "header",
		"content": "Section: 2"
	  },
	  {
		"type": "checkbox",
		"id": "enable_second_section_video",
		"label": "Show video",
		"default": false
	  },
	  {
		"type": "url",
		"id": "second_section_video",
		"label": "Video URL"
	  },
	  {
		"type": "image_picker",
		"id": "second_section_image_d",
		"label": "Image (Desktop)"
	  },
	  {
		"type": "image_picker",
		"id": "second_section_image_m",
		"label": "Image (Mobile)"
	  },
	  {
		"type": "text",
		"id": "second_section_heading",
		"label": "Heading",
		"default": "recycling is free"
	  },
	  {
		"type": "richtext",
		"id": "second_section_paragraph",
		"label": "Paragraph",
		"default": "<p>recycle from home by joining the program - save your packaging, download a free shipping label, and send in your empties or search the map for a local recycling solution</p>"
	  },
	  {
		"type": "text",
		"id": "second_section_btn1",
		"label": "Button text",
		"default": "print your shipping label"
	  },
	  {
		"type": "url",
		"id": "second_section_btn1_url",
		"label": "Button link"
	  },
	  {
		"type": "text",
		"id": "second_section_btn2",
		"label": "Button text",
		"default": "see dropoffs near you"
	  },
	  {
		"type": "url",
		"id": "second_section_btn1_ur2",
		"label": "Button link"
	  },
	  {
		"type": "header",
		"content": "Section: 3"
	  },
	  {
		"type": "checkbox",
		"id": "enable_third_section_video",
		"label": "Show video",
		"default": false
	  },
	  {
		"type": "url",
		"id": "third_section_video",
		"label": "Video URL"
	  },
	  {
		"type": "image_picker",
		"id": "third_section_image_d",
		"label": "Image (Desktop)"
	  },
	  {
		"type": "image_picker",
		"id": "third_section_image_m",
		"label": "Image (Mobile)"
	  },
	  {
		"type": "text",
		"id": "third_section_heading",
		"label": "Heading",
		"default": "What is TerraCycle?"
	  },
	  {
		"type": "richtext",
		"id": "third_section_paragraph",
		"label": "Paragraph",
		"default": "<p>TerraCycle is a social enterprise dedicated to eliminating the idea of waste that specializes in recycling the un-recyclable. Ever look at a product and think, can this be recycled…? Well TerraCycle removes that worry and recycles your empties into something new. They collect, store, and recycle everything from our shampoo bottles to our spray pumps.</p>"
	  },
	  {
		"type": "header",
		"content": "Section: 4"
	  },
	  {
		"type": "text",
		"id": "fourth_section_heading",
		"label": "Heading",
		"default": "how it works"
	  },
	  {
		"type": "paragraph",
		"content": "Use the blocks to add the contents"
	  },
	  {
		"type": "header",
		"content": "Section: 5"
	  },
	  {
		"type": "checkbox",
		"id": "enable_fifth_section_video",
		"label": "Show video",
		"default": false
	  },
	  {
		"type": "url",
		"id": "fifth_section_video",
		"label": "Video URL"
	  },
	  {
		"type": "image_picker",
		"id": "fifth_section_image_d",
		"label": "Image (Desktop)"
	  },
	  {
		"type": "image_picker",
		"id": "fifth_section_image_m",
		"label": "Image (Mobile)"
	  },
	  {
		"type": "richtext",
		"id": "fifth_section_text",
		"label": "Text"
	  }
	],
	"blocks": [
	  {
		"type": "how-it-works",
		"name": "how it works",
		"limit": 4,
		"settings": [
		  {
			"type": "checkbox",
            "id": "show_video",
            "label": "Show video"
		  },
		  {
			"type": "url",
            "id": "video_url",
            "label": "Video URL"
		  },
		  {
            "type": "image_picker",
            "id": "image",
            "label": "Image"
          },
          {
            "type": "text",
            "id": "heading",
            "label": "Heading"
          },
          {
            "type": "richtext",
            "id": "paragraph",
            "label": "Paragraph"
		  }
		]
	  }
	]
  }
{% endschema %}